  "version": "0.2.0",
  "configurations": [

    {
      "name": "liferay version",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["--version"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay config list",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["config", "list"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime create",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "create"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime delete",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "delete"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime mkcert",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "mkcert"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime mkcert --install",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "mkcert", "--install"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime kubeconfig",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "kubeconfig"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime status",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "status"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime status -v",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "status", "-v"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime stop",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "stop"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay runtime stop -v",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "stop", "-v"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay ext create",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "create"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay ext create -d foo",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "create", "-d", "foo"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay ext refresh -v",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "refresh", "-v"],
      "cwd": "${input:workspacePath}",
      "console": "integratedTerminal"
    },
    {
      "name": "liferay ext start",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "start"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay ext start -b",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "start", "-b"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay ext start -v",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "start", "-v"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay ext status",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "status"],
      "console": "integratedTerminal"
    },
    {
      "name": "liferay ext stop",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "stop"],
      "console": "integratedTerminal"
    }
  ],
  "inputs": [
    {
      "id": "workspacePath",
      "description": "Enter the path to client extensions workspace",
      "type": "promptString"
    }
  ]
}