  "version": "0.2.0",
  "configurations": [

    {
      "name": "liferay version",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["--version"]
    },
    {
      "name": "liferay config list",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["config", "list"]
    },
    {
      "name": "liferay runtime create",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "create"]
    },
    {
      "name": "liferay runtime delete",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "delete"]
    },
    {
      "name": "liferay runtime mkcert",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "mkcert"]
    },
    {
      "name": "liferay runtime mkcert --install",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "mkcert", "--install"]
    },
    {
      "name": "liferay runtime kubeconfig",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "kubeconfig"]
    },
    {
      "name": "liferay runtime status",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "status"]
    },
    {
      "name": "liferay runtime status -v",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "status", "-v"]
    },
    {
      "name": "liferay runtime stop",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "stop"]
    },
    {
      "name": "liferay runtime stop -v",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["runtime", "stop", "-v"]
    },
    {
      "name": "liferay ext create",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "create"]
    },
    {
      "name": "liferay ext create -d foo",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "create", "-d", "foo"]
    },
    {
      "name": "liferay ext refresh -v",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "refresh", "-v"],
      "cwd": "${input:workspacePath}"
    },
    {
      "name": "liferay ext start",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "start"]
    },
    {
      "name": "liferay ext start -b",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "start", "-b"]
    },
    {
      "name": "liferay ext start -v",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "start", "-v"]
    },
    {
      "name": "liferay ext status",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "status"]
    },
    {
      "name": "liferay ext stop",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "args": ["ext", "stop"]
    }
  ],
  "inputs": [
    {
      "id": "workspacePath",
      "description": "Enter the path to client extensions workspace",
      "type": "promptString"
    }
  ]
}